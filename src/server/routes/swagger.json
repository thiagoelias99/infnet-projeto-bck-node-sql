{
  "openapi": "3.0.3",
  "info": {
    "title": "API Infnet Projeto Backend Com Node + Express + Sql",
    "description": "Api desenvolvida para o curso Backend com Node e SQL da InfNet",
    "version": "0.0.1",
    "contact": {
      "name": "Thiago Elias",
      "email": "thiagoelias99@gmail.com",
      "url": "https://www.linkedin.com/in/eng-thiagoelias/"
    }
  },
  "externalDocs": {
    "description": "Repositório do Projeto",
    "url": "https://github.com/thiagoelias99/infnet-projeto-bck-node-sql"
  },
  "servers": [
    {
      "url": "http://localhost:3333"
    }
  ],
  "paths": {
    "/login": {
      "post": {
        "summary": "Login and returns JWT",
        "description": "Login user by email and password. Returns JWT for authentication",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "jwt": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/students/info": {
      "get": {
        "summary": "Returns current student data",
        "description": "Returns current student info and courses. Require JWT authentication",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StudentWCourses"
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    },
    "/students": {
      "post": {
        "summary": "Create a Student",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StudentCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "uuid": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "get": {
        "summary": "Returns all students",
        "description": "Returns all students. Require JWT admin level",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Student"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    },
    "/students/{uuid}": {
      "parameters": [
        {
          "name": "uuid",
          "in": "path",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "get": {
        "summary": "Return a student by UUID",
        "description": "Returns a student by UUID. Require JWT admin level",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/StudentWCourses"
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      },
      "put": {
        "summary": "Update student by UUID",
        "description": "Update a student by UUID. Require JWT admin level",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StudentCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      },
      "delete": {
        "summary": "Delete student by UUID",
        "description": "Delete a student by UUID. Require JWT admin level",
        "responses": {
          "200": {
            "description": "Success"
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    },
    "/courses/info": {
      "get": {
        "summary": "Returns courses available",
        "description": "Returns courses available for subscription. Require JWT authentication",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CourseComplete"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    },
    "/courses": {
      "post": {
        "summary": "Create a Course",
        "description": "Create a course. Require JWT admin level",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CourseCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "uuid": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      },
      "get": {
        "summary": "Returns all courses",
        "description": "Returns all courses and students subscribed. Require JWT",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CourseComplete"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    },
    "/courses/{uuid}": {
      "parameters": [
        {
          "name": "uuid",
          "in": "path",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "get": {
        "summary": "Return course UUID",
        "description": "Return a course by UUID. Require JWT",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CourseComplete"
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      },
      "put": {
        "summary": "Update course by UUID",
        "description": "Update a course by UUID. Require JWT admin level",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CourseCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      },
      "delete": {
        "summary": "Delete course by UUID",
        "description": "Delete a course by UUID. Require JWT admin level",
        "responses": {
          "204": {
            "description": "Success. No Content"
          },
          "400": {
            "$ref": "#/components/responses/400"
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    },
    "/courses/{uuid}/subscribe": {
      "parameters": [
        {
          "name": "uuid",
          "in": "path",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "patch": {
        "summary": "Subscribe student to course",
        "description": "Subscribe student to course UUID. Use student UUID from JWT",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CourseComplete"
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    },
    "/courses/{uuid}/unsubscribe": {
      "parameters": [
        {
          "name": "uuid",
          "in": "path",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "patch": {
        "summary": "Unsubscribe student from the course",
        "description": "Unsubscribe student from the course UUID. Use student UUID from JWT",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CourseComplete"
                }
              }
            }
          }
        },
        "security": [
          {
            "auth": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "Student": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "birthDate": {
            "type": "string"
          }
        }
      },
      "Login": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "thiago@email.com"
          },
          "password": {
            "type": "string",
            "example": "123456"
          }
        }
      },
      "StudentCreate": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Thiago Elias"
          },
          "email": {
            "type": "string",
            "example": "thiago@email.com"
          },
          "password": {
            "type": "string",
            "example": "123456"
          },
          "birthDate": {
            "type": "string",
            "example": "09/15/2000"
          }
        }
      },
      "StudentWCourses": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "birthDate": {
            "type": "string"
          },
          "Courses": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Course"
            }
          }
        }
      },
      "Course": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "courseHours": {
            "type": "number"
          },
          "startDate": {
            "type": "string"
          },
          "finishDate": {
            "type": "string"
          }
        }
      },
      "CourseCreate": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string",
            "example": "Curso de Backend - Nível 1"
          },
          "courseHours": {
            "type": "number",
            "example": 20
          },
          "startDate": {
            "type": "string",
            "example": "07/01/2023"
          },
          "finishDate": {
            "type": "string",
            "example": "08/01/2023"
          }
        }
      },
      "CourseComplete": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "courseHours": {
            "type": "number"
          },
          "startDate": {
            "type": "string"
          },
          "finishDate": {
            "type": "string"
          },
          "Students": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "uuid": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                }
              }
            }
          },
          "numberOfSubscribers": {
            "type": "number"
          },
          "courseStatus": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "auth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "responses": {
      "400": {
        "description": "Error: Bad Request",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      "401": {
        "description": "Error: Unauthorized",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      "500": {
        "description": "Internal Server Error",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
}