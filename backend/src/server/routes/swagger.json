{
  "openapi": "3.0.3",
  "info": {
    "title": "API de Exemplo",
    "description": "Exemplo de utilização do Swagger",
    "version": "0.0.1",
    "termsOfService": "http://www.google.com",
    "contact": {
      "name": "Thiago Elias",
      "email": "thiagoelias99@gmail.com",
      "url": "https://www.linkedin.com/in/eng-thiagoelias/"
    },
    "license": {
      "name": "License: GPLv3",
      "url": "https://www.gnu.org/licenses/quick-guide-gplv3.pt-br.html"
    }
  },
  "externalDocs": {
    "description": "Documentação formal",
    "url": "http://www.google.com"
  },
  "servers": [
    {
      "url": "http://localhost:3333"
    }
  ],
  "paths": {
    "/students": {
      "get": {
        "summary": "Retorna todos os estudantes",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "uuid": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "email": {
                        "type": "string"
                      },
                      "birthDate": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/students/{uuid}": {
      "parameters": [
        {
          "name": "uuid",
          "in": "path",
          "schema": {
            "type": "string"
          },
          "required": true
        }
      ],
      "get": {
        "summary": "Retorna estudante",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "uuid": {
                      "type": "string"
                    },
                    "name": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "birthDate": {
                      "type": "string"
                    },
                    "Courses": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "uuid": {
                            "type": "string"
                          },
                          "description": {
                            "type": "string"
                          },
                          "courseHours": {
                            "type": "number"
                          },
                          "startDate": {
                            "type": "string"
                          },
                          "finishDate": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Authorization not found in request header"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}